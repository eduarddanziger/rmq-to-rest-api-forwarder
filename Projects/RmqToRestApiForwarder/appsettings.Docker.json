{
    "RabbitMQ": {
        "Service": {
            "HostName": "rabbitmq",
            "UserName": "forwarder",
            "Password": "forwarder"
        }
    },
    "NLog": {
        "variables": {
            "logdir": "/app/logs",
            "logname": "RmqToRestApiForwarder"
        },
        "targets": {
            "all-file": {
                "layout": "${date:format=yyyy-MM-dd HH\\:mm\\:ss.ffffff} ${level:uppercase=true:format=FirstCharacter} [${native-thread-id}] ${message}"
            },
            "own-console": {
                "target": {
                    "layout": "${date:format=yyyy-MM-dd HH\\:mm\\:ss.ffffffzzz} [${level:lowercase=true}] ${message}"
                }
            }
        }
    }
}
